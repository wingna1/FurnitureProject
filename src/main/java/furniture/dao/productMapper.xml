<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="productSQL">
 
	<insert id="productQnAWrite" parameterType="qna">
		insert into product_qna values(seq_productQna.nextval, #{email}, #{product_id}, #{qna_subject}, #{qna_content}, sysdate, 0)
	</insert>
	
	<select id="productQnAList" resultType="qna">
		select * from product_qna
	</select>
	
 </mapper>